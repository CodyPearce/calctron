{
  "name": "calctron",
  "productName": "Calctron",
  "version": "1.0.0",
  "description": "A beautiful desktop calculator app built on Electron",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "asar pack calctron-linux-x64/resources/app calctron-linux-x64/resources/app.asar",
